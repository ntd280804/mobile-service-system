@model WebApp.Areas.Admin.Controllers.ImportController.ImportDetailViewModel

<h2>Chi tiết Import #@Model.StockInId</h2>

<div>
    <p><strong>Employee Name:</strong> @Model.EmpUsername</p>
    <p><strong>In Date:</strong> @Model.InDate</p>
    <p><strong>Note:</strong> @Model.Note</p>
</div>

<h3>Items</h3>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Part Name</th>
            <th>Manufacturer</th>
            <th>Serial</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items)
        {
            <tr>
                <td>@item.PartName</td>
                <td>@item.Manufacturer</td>
                <td>@item.Serial</td>
                <td>@item.Price</td>
            </tr>
        }
    </tbody>
</table>
